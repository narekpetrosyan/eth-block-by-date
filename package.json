{
  "name": "eth-block-by-date",
  "version": "1.0.6",
  "description": "Get Ethereum block number by a given date.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --detectOpenHandles --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/narekpetrosyan/eth-block-by-date.git"
  },
  "keywords": [
    "ethereum",
    "blockchain",
    "blockNumber",
    "timestamp"
  ],
  "author": "Narek Petrosyan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/narekpetrosyan/eth-block-by-date/issues"
  },
  "homepage": "https://github.com/narekpetrosyan/eth-block-by-date#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "ethers": "^5.7.2",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "dayjs": "^1.11.7"
  }
}
